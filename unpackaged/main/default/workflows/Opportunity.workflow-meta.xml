    <rules>
        <fullName>Push Counter</fullName>
        <actions>
            <name>Increment_Push_Counter_Field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Increment the Push Counter field by 1</description>
        <formula>IF( CloseDate &gt; PRIORVALUE( CloseDate ), IF (MONTH(CloseDate) &lt;&gt; MONTH(PRIORVALUE( CloseDate )) , TRUE, FALSE), FALSE)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Sent Customer Survey - notify contact</fullName>
        <actions>
            <name>Service_Opportunity_Close_notify_contact</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>Generate an email to Contact when Service Opportunity Stage = Won</description>
        <formula>Record_Type__c = &#39;Services Only / Consulting request&#39; 
&amp;&amp; TEXT(StageName) =&#39;Stage 9 - Won&#39; 
&amp;&amp; TEXT(PRIORVALUE(Scheduled_In_Clarity__c)) &lt;&gt; &#39;Assignment Complete&#39; 
&amp;&amp; ISPICKVAL(Scheduled_In_Clarity__c, &#39;Assignment Complete&#39;) 
&amp;&amp; NOT ISBLANK(  Customer_Contact_Email__c  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update ASM</fullName>
        <actions>
            <name>Update_ASM</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Update ASM for no pricebook overwritten</description>
        <formula>NOT (IsClosed) &amp;&amp;  NOT (Pricebook_Overridden__c) &amp;&amp;   Record_Type__c = &#39;Licence Fee&#39;</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Annual Licence Fee</fullName>
        <actions>
            <name>Update_Annual_Licence_Fee</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.IsClosed</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Licence Fee</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Licence_Model__c</field>
            <operation>equals</operation>
            <value>SaaS,SaaS Flip</value>
        </criteriaItems>
        <description>Update Annual Licence Fee for an Open SaaS/SaaS Flip Opportunity</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Bizible Opportunity Amount</fullName>
        <actions>
            <name>Bizible_Opp_Amount</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.ARR1__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Estimated Daily Travel Charge</fullName>
        <actions>
            <name>Update_Estimated_Travel_Charge</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Will default value from account once saved.</description>
        <formula>NOT ISBLANK(Account.Estimated_Daily_Travel_Charge__c)  &amp;&amp; ISBLANK(Estimated_Daily_Travel_Charge__c)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Update Opportunity Amount - Service%2FConsulting</fullName>
        <actions>
            <name>Update_Opportunity_Amount_Service_Cons</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update Opportunity Amount - for record type - Service/Consulting</description>
        <formula>Record_Type__c = &#39;Services Only / Consulting request&#39;
&amp;&amp; ISCHANGED(Total_Service_Consulting_Fee__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update SaaS Term Avg Year</fullName>
        <actions>
            <name>Update_SaaS_Term_Avg_Year</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Update SaaS Term Avg Year based on SaaS Term</description>
        <formula>(TEXT(PRIORVALUE(SaaS_Term__c)) &lt;&gt; TEXT(SaaS_Term__c) 
|| ISNEW() )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Sent Customer Survey</fullName>
        <actions>
            <name>Update_Sent_Customer_Survey</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Tick Sent Customer Survey check box when Closed Won and Assignment complete</description>
        <formula>Record_Type__c = &#39;Services Only / Consulting request&#39; 
&amp;&amp; Not (Send_Feedback_Survey__c) 
&amp;&amp;  ISPICKVAL(StageName, &#39;Stage 9 - Won&#39;) 
&amp;&amp;  ISPICKVAL(Scheduled_In_Clarity__c,&#39;Assignment complete&#39;)
&amp;&amp; TEXT(PRIORVALUE(Scheduled_In_Clarity__c)) &lt;&gt; &#39;Assignment Complete&#39; 
&amp;&amp; NOT ISBLANK( Customer_Contact_Email__c )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Stage Modified Date</fullName>
        <actions>
            <name>Update_Stage_Modified_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED( StageName)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <tasks>
