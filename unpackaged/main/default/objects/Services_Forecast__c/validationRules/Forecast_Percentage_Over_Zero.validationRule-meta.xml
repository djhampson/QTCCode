<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Forecast_Percentage_Over_Zero</fullName>
    <active>true</active>
    <description>Forecast Percentage must be over 0</description>
    <errorConditionFormula>Forecast__c &lt;=0</errorConditionFormula>
    <errorDisplayField>Forecast__c</errorDisplayField>
    <errorMessage>The Forecast percentage must be over 0</errorMessage>
</ValidationRule>
