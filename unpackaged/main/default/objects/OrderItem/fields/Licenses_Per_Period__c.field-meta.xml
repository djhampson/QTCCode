<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Licenses_Per_Period__c</fullName>
    <description>Number of Licenses per period</description>
    <externalId>false</externalId>
    <formula>ROUND(((EndDate  -  ServiceDate) / 365 * 12),0) * Quantity</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of Licenses per period</inlineHelpText>
    <label>Licenses Per Period</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
